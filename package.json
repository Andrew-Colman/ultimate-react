    "url": "https://www.spectralapps.com.br"
  },
  "keywords": [
    "react",
    "ultimate react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/spectralapps/ultimate-react"
  },
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "start": "parcel serve ./src/index.html --open",
    "build": "parcel build ./src/index.html",
    "bundle_analyze": "set PARCEL_BUNDLE_ANALYZER=1 && parcel build ./src/index.html --dist-dir ./build-report",
    "postbundle_analyze": "html-pages -d ./parcel-bundle-reports/default.html -o -S",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "test": "echo no test implemented yet, add a test runner manually"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "html-pages": "^3.1.0",
    "parcel": "^2.0.0-beta.1",
    "prettier": "^2.0.5"
  }
}
